###  https://w3id.org/IECCIM/TC57CIM/IEC61970/Base/LoadModel#EnergyArea
:EnergyArea rdf:type owl:Class ;
            rdfs:subClassOf <https://w3id.org/IECCIM/TC57CIM/IEC61970/Base/Core#IdentifiedObject> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :EnergyArea_ControlArea ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass :EnergyArea
