###  https://w3id.org/IECCIM/TC57CIM/IEC61968/InfIEC61968/InfERPSupport#BillMediaKind
:BillMediaKind rdf:type rdfs:Datatype ;
               owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                     owl:oneOf [ rdf:type rdf:List ;
                                                 rdf:first "electronic" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "other" ;
                                                            rdf:rest [ rdf:type rdf:List ;
                                                                       rdf:first "paper" ;
                                                                       rdf:rest rdf:nil
                                                                     ]
                                                          ]
                                               ]
                                   ] .


###  https://w3id.org/IECCIM/TC57CIM/IEC61968/InfIEC61968/InfERPSupport#ErpAccountKind
:ErpAccountKind rdf:type rdfs:Datatype ;
                owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                      owl:oneOf [ rdf:type rdf:List ;
                                                  rdf:first "estimate" ;
                                                  rdf:rest [ rdf:type rdf:List ;
                                                             rdf:first "normal" ;
                                                             rdf:rest [ rdf:type rdf:List ;
                                                                        rdf:first "reversal" ;
                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                   rdf:first "statistical" ;
                                                                                   rdf:rest rdf:nil
                                                                                 ]
                                                                      ]
                                                           ]
                                                ]
                                    ] .


###  https://w3id.org/IECCIM/TC57CIM/IEC61968/InfIEC61968/InfERPSupport#ErpInvoiceKind
:ErpInvoiceKind rdf:type rdfs:Datatype ;
                owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                      owl:oneOf [ rdf:type rdf:List ;
                                                  rdf:first "purchase" ;
                                                  rdf:rest [ rdf:type rdf:List ;
                                                             rdf:first "sale" ;
                                                             rdf:rest rdf:nil
                                                           ]
                                                ]
                                    ] .


###  https://w3id.org/IECCIM/TC57CIM/IEC61968/InfIEC61968/InfERPSupport#ErpInvoiceLineItemKind
:ErpInvoiceLineItemKind rdf:type rdfs:Datatype ;
                        owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                              owl:oneOf [ rdf:type rdf:List ;
                                                          rdf:first "initial" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "other" ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "recalculation" ;
                                                                                rdf:rest rdf:nil
