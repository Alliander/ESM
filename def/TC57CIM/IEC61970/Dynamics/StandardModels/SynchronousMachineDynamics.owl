###  https://w3id.org/netbeheer/IECCIM/TC57CIM/IEC61970/Dynamics/StandardModels/SynchronousMachineDynamics#IfdBaseKind
:IfdBaseKind rdf:type rdfs:Datatype ;
             owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                   owl:oneOf [ rdf:type rdf:List ;
                                               rdf:first "ifag" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "iffl" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "ifnl" ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "other" ;
                                                                                rdf:rest rdf:nil
                                                                              ]
                                                                   ]
                                                        ]
                                             ]
                                 ] .


###  https://w3id.org/netbeheer/IECCIM/TC57CIM/IEC61970/Dynamics/StandardModels/SynchronousMachineDynamics#RotorKind
:RotorKind rdf:type rdfs:Datatype ;
           owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "roundRotor" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "salientPole" ;
                                                        rdf:rest rdf:nil
                                                      ]
                                           ]
                               ] .


###  https://w3id.org/netbeheer/IECCIM/TC57CIM/IEC61970/Dynamics/StandardModels/SynchronousMachineDynamics#SynchronousMachineModelKind
:SynchronousMachineModelKind rdf:type rdfs:Datatype ;
                             owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                                   owl:oneOf [ rdf:type rdf:List ;
                                                               rdf:first "subtransient" ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "subtransientSimplified" ;
                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                     rdf:first "subtransientSimplifiedDirectAxis" ;
                                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                                rdf:first "subtransientTypeF" ;
                                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                                           rdf:first "subtransientTypeJ" ;
                                                                                                           rdf:rest rdf:nil
                                                                                                         ]
                                                                                              ]
                                                                                   ]
                                                                        ]
                                                             ]
                                                 ] .


